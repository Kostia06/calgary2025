generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid()) @db.Uuid
  clerk_id  String   @unique
  email     String   @unique
  fullname  String
  avatar_url String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
}
